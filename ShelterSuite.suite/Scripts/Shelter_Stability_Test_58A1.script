//Motorola Shelter Stability Test 58 -- Make a phone call
//Component of Script Series 58
//Completed 6-10-2013

//Setup
run ClassNavigate.ResetState

//Subroutine A1 -- Shelter Device A - Dial Shelter Device B
Click "Device/AppIcons/TelephoneApp"
Click "Device/Apps/Telephone/PhoneIcon"
Click "Device/Apps/Telephone/PhoneDial7"
Click "Device/Apps/Telephone/PhoneDial2"
Click "Device/Apps/Telephone/PhoneDial0"
Click "Device/Apps/Telephone/PhoneDial3"
Click "Device/Apps/Telephone/PhoneDial6"
Click "Device/Apps/Telephone/PhoneDial0"
Click "Device/Apps/Telephone/PhoneDial7"
Click "Device/Apps/Telephone/PhoneDial2"
Click "Device/Apps/Telephone/PhoneDial1"
Click "Device/Apps/Telephone/PhoneDial4"
run ClassUtilities.ConfirmTextInArea("(720) 360-7214"),("10,110"),("450,250")
Click "Device/Apps/Telephone/StartCall"
run ClassUtilities.ConfirmTextOnScreen("Dialing")
run ClassUtilities.ConfirmTextOnScreen("ShelterB")
